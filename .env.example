# Firebase Configuration
# Android
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here
FIREBASE_ANDROID_MESSAGING_SENDER_ID=your_messaging_sender_id_here

# iOS (optional - can use Android values as fallback)
FIREBASE_IOS_API_KEY=
FIREBASE_IOS_APP_ID=
FIREBASE_IOS_BUNDLE_ID=

# macOS (optional - can use Android values as fallback)
FIREBASE_MACOS_API_KEY=
FIREBASE_MACOS_APP_ID=
FIREBASE_MACOS_BUNDLE_ID=

# Windows (optional - can use Android values as fallback)
FIREBASE_WINDOWS_API_KEY=
FIREBASE_WINDOWS_APP_ID=

# Linux (optional - can use Android values as fallback)
FIREBASE_LINUX_API_KEY=
FIREBASE_LINUX_APP_ID=

# Web
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_MEASUREMENT_ID=

# Common
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
